{
  "name": "jss-theme",
  "version": "0.3.2",
  "description": "JSS theming solution which could be used with any view library or framework",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "update": "./node_modules/.bin/ts-node updateReadme.ts",
    "build": "npm run lint && rm -rf lib && npm run update && ./node_modules/.bin/tsc",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "jss",
    "theming",
    "angular",
    "jss-theme",
    "theme"
  ],
  "author": "Danyil Moroz",
  "license": "MIT",
  "dependencies": {
    "jss": "^10.0.4",
    "jss-preset-default": "^10.0.4",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "ts-node": "^8.6.2",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mopcweb/jss-theme"
  },
  "homepage": "https://github.com/mopcweb/jss-theme"
}
